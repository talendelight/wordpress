# Production Environment Variables
# Copy this file to .env and fill in your secure values
# NEVER commit the .env file to version control

# Database Configuration
MYSQL_ROOT_PASSWORD=your_secure_root_password_here
MYSQL_USER=wordpress_user
MYSQL_PASSWORD=your_secure_wordpress_db_password_here
MYSQL_DATABASE=wordpress

# WordPress Configuration
WORDPRESS_DB_NAME=wordpress
WORDPRESS_TABLE_PREFIX=wp_
WORDPRESS_DB_USER=wordpress_user
WORDPRESS_DB_PASSWORD=your_secure_wordpress_db_password_here
WORDPRESS_DEBUG=0

# Note: Ensure WORDPRESS_DB_PASSWORD and MYSQL_PASSWORD match
# Note: These passwords should be strong and unique for production
